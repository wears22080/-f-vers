local function saveStateToFile()
    local file = io.open('auto_su_state.txt', 'w')
    if file then
        file:write(tostring(radioRequestEnabled.value))
        file:close()
    end
end

local function loadStateFromFile()
    local file = io.open('auto_su_state.txt', 'r')
    if file then
        local state = file:read('*all')
        radioRequestEnabled.value = state == 'true'
        file:close()
    end
end

function checkVisibleArticles(articlesArray)
    for _, article in ipairs(articlesArray) do
        if searchString.v == '' or article.text:lower():find(searchString.v:lower()) then
            return true
        end
    end
    return false
end


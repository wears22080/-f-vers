function checkVisibleArticles(chapter, isVisible, headerText)
    local anyArticleVisible = false

    if isVisible then
        for _, article in ipairs(chapter) do
            if searchString.v == '' or article.text:lower():find(searchString.v:lower()) then
                anyArticleVisible = true
                break
            end
        end
    end

    if isVisible and anyArticleVisible then
        if imgui.CollapsingHeader(headerText) then
            for _, article in ipairs(chapter) do
                if searchString.v == '' or article.text:lower():find(searchString.v:lower()) then
                    if imgui.Selectable(article.text) then
                        if radioRequestEnabled.value then
                            messagesQueue = { 
                                string.format('/r Запрашиваю розыск на дело: "%s" степень розыска: "%d" с причиной: "%s"', selectedId, article.penalty, article.code),
                                '/r Доказательства есть на боди-камере!'
                            }
                        else
                            messagesQueue = { 
                                '/me взял КПК, авторизовался и вошел в систему',
                                '/me выбрал пункт "обьявить в розыск" после чего указал приметы нарушителя',
                                '/me выключил КПК и положил его на место',
                                string.format('/su %d %d %s', selectedId, article.penalty, article.code)
                            }
                        end
                        nextMessageTime = os.clock() 
                        showMenu = false
                    end
                end
            end
        end
    end
end


function main()
    if not isSampLoaded() or not isSampfuncsLoaded() then return end
    while not isSampAvailable() do wait(100) end

    sampRegisterChatCommand('update', cmd_update)

    _, id = sampGetPlayerIdByCharHandle(PLAYER_PED)
    nick = sampGetPlayerNickname(id)

    downloadUrlToFile(update_url, update_path, function(id, status)
        if status == dlstatus.STATUS_ENDDOWNLOADDATA then
            local updateIni = inicfg.load(nil, update_path)
            if tonumber(updateIni.info.vers) > script_vers then
                sampAddChatMessage('Есть обновления! Версия: ' .. updateIni.info.vers_text, -1)
                update_state = true
            end
            os.remove(update_path)
        end
    end)

    apply_custom_style() 
    local serverName = sampGetCurrentServerName()
    if serverName then
        local message = string.format("{FFD700}[Auto Su] {B3E0FF}Подключились к серверу.")
        sampAddChatMessage(utf8_to_cp1251:iconv(message), -1)
    end

    sampRegisterChatCommand('su', suCommand)
    imgui.ToggleCursor = true
    loadStateFromFile()
    activeButton = radioRequestEnabled.value and "Вкл" or "Выкл"

    while true do
        wait(0)

        if nextMessageTime > 0 and os.clock() > nextMessageTime and #messagesQueue > 0 then
            local message = table.remove(messagesQueue, 1)
            sampSendChat(utf8_to_cp1251:iconv(message))
            nextMessageTime = os.clock() + 2.5
        end

        if showMenu and showSearchWindow then
            imgui.Process = true
        else
            imgui.Process = false
        end

        if update_state then
            downloadUrlToFile(script_url, script_path, function(id, status)
                if status == dlstatus.STATUS_ENDDOWNLOADDATA then
                    sampAddChatMessage('Скрипт обновлен!', -1)
                    thisScript():reload()
                end
            end)
            break
        end
    end
end

function checkAllVisibleArticles()
    for _, data in ipairs(chapters) do
        checkVisibleArticles(data.chapter, data.show, data.header)
    end
end

function suCommand(arg)
    selectedId = tonumber(arg)
    if selectedId then
        if selectedId >= 0 and selectedId <= 1000 then
            if sampIsPlayerConnected(selectedId) then
                showMenu = not showMenu
            else
                local message = utf8_to_cp1251:iconv('{ff0000}Ошибка:{FFFFFF} Игрока с таким ID нет на сервере.')
                sampAddChatMessage(message, -1)
            end
        else
            local message = utf8_to_cp1251:iconv('{ff0000}Ошибка:{FFFFFF} ID игрока должно быть от 0 до 1000.')
            sampAddChatMessage(message, -1)
        end
    else
        local message = utf8_to_cp1251:iconv('{ff0000}Ошибка:{FFFFFF} Введите корректный ID игрока.')
        sampAddChatMessage(message, -1)
    end
end

